    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "build/shaders/*.spv": true,
    },
    "editor.insertSpaces": false,
    "editor.renderWhitespace": "boundary",
    "editor.formatOnSave": false,
    "editor.mouseWheelZoom": true,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false
    },
    "editor.quickSuggestionsDelay": 10,
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "all",
    "editor.showFoldingControls": "mouseover",
    "editor.smoothScrolling": true,
    "editor.stablePeek": false,
    "editor.tabCompletion": "off",
    "editor.wordBasedSuggestions": false,
    "editor.wordWrapColumn": 200,
    "workbench.statusBar.feedback.visible": false,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": false,
    "files.useExperimentalFileWatcher": false,
    "explorer.openEditors.visible": 0,
    "debug.allowBreakpointsEverywhere": true,
    "debug.enableAllHovers": true,
    "debug.inlineValues": true,
    "debug.openExplorerOnEnd": true,
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "C_Cpp.workspaceParsingPriority": "high",
    "C_Cpp.workspaceSymbols": "All",
    "c-cpp-flylint.enable": false,
    "c-cpp-flylint.debug": false,
    "c-cpp-flylint.flexelint.enable": false,
    "c-cpp-flylint.clang.enable": true,
    "c-cpp-flylint.clang.standard": ["c++17"],
    "c-cpp-flylint.standard":[
        "c++17"
    ],
    "glsllint.glslangValidatorPath": "/usr/bin/glslangValidator",
}
